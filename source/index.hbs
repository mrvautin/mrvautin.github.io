<div class="row">
  <nav id="sidebarMenu" class="col-md-3 col-lg-2 d-md-block bg-light sidebar collapse">
    <div class="position-sticky pt-3">
      <ul class="nav flex-column">
        {{#each postList}}
        <li class="nav-item">
          <a class="nav-link" href="/{{this.permalink}}">
            {{this.title}}
          </a>
        </li>
        {{/each}}
      </ul>
    </div>
  </nav>

  <main class="col-md-9 ms-sm-auto col-lg-10 mt-3 px-md-4"><div class="chartjs-size-monitor"><div class="chartjs-size-monitor-expand"><div class=""></div></div><div class="chartjs-size-monitor-shrink"><div class=""></div></div></div>
    <h1>{{posts.1.title}}</h1>
    {{{posts.1.body}}}
  </main>
</div>